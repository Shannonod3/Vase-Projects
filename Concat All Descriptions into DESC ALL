SET NOCOUNT OFF
GO
SELECT DISTINCT LC.BZLY_VaseNbr
	, LC.FLD02 AS "Description_All"
--INTO ##D
FROM LandingZone.Canidate AS "LC"
	JOIN Staging.VaseParsing AS "VP" 
		ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
where LC.FLD02 LIKE '%:%'AND  LC.FLD02 NOT LIKE '%by%'
	AND LC.FLD02 NOT IN (SELECT Input FROM dbo.LU_Geography)
	AND ISNUMERIC(LEFT(LC.FLD02,1))=0
	AND LC.FLD02 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD03 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD03 LIKE '%:%'AND  LC.FLD03 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD03,1))=0
		AND LC.FLD03 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD04 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD04 LIKE '%:%'AND  LC.FLD04 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD04,1))=0
		AND LC.FLD04 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD05 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD05 LIKE '%:%'AND  LC.FLD05 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD05,1))=0
		AND LC.FLD05 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD06 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD06 LIKE '%:%'AND  LC.FLD06 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD06,1))=0
		AND LC.FLD06 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD07 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD07 LIKE '%:%'AND  LC.FLD07 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD07,1))=0
		AND LC.FLD07 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD08 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD08 LIKE '%:%'AND  LC.FLD08 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD08,1))=0
		AND LC.FLD08 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD09 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD09 LIKE '%:%'AND  LC.FLD09 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD09,1))=0
		AND LC.FLD09 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD10 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD10 LIKE '%:%'AND  LC.FLD10 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD10,1))=0
		AND LC.FLD10 NOT LIKE 'Inscription%'
UNION 
	SELECT DISTINCT LC.BZLY_VaseNbr
		, LC.FLD11 AS "Description_All"
	FROM LandingZone.Canidate AS "LC"
		JOIN Staging.VaseParsing AS "VP" 
			ON LC.BZLY_VaseNbr = VP.BZLY_VaseNbr
	where LC.FLD11 LIKE '%:%'AND  LC.FLD11 NOT LIKE '%by%'
		AND ISNUMERIC(LEFT(LC.FLD11,1))=0
		AND LC.FLD11 NOT LIKE 'Inscription%'
